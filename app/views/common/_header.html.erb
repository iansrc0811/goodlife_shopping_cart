<div class="agileits_header">
  <div class="container">
    <div class="w3l_offers">
      <p><a href="/">SHOP NOW</a></p>
    </div>
    <div class="agile-login">
      <ul>
        <% if current_user %>
          <li><%= link_to('Logout', destroy_user_session_path, :method => :delete) %> </li>
          <li><%= link_to('Change Password', edit_registration_path(:user)) %></li>
        <% else %>
          <li><%= link_to('Create Account', new_registration_path(:user)) %> </li>
          <li><%= link_to('Login', new_session_path(:user)) %> </li>
        <% end %>
      </ul>
    </div>
    <% if current_user %>
      <div class="product_list_header">
        <button class="w3view-cart" name="submit" type="submit" value="">
          <i aria-hidden="true" class="fa fa-cart-arrow-down" data-target="#cartModal" data-toggle="modal"></i>
        </button>
      </div>
    <% end %>
    <div class="clearfix"></div>
  </div>
</div>
